{
  "name": "angular_core",
  "version": "0.0.3",
  "private": true,
  "license": "UNLISENSED",
  "browserslist": [
    "last 3 chrome versions",
    "last 3 firefox versions",
    "last 3 opera versions",
    "last 3 edge versions",
    "last 3 safari versions",
    "ie >= 11",
    "ios >= 11",
    "android >= 4.4.2",
    "ie_mob >= 10",
    "last 3 firefoxandroid versions",
    "last 3 chromeandroid versions"
  ],
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./set-env.ts",
    "start": "npm run config -- --environment=dev && ng serve --open",
    "serve:build": "npm run config && ng serve --prod --open",
    "build": "npm run config && ng build --prod --aot --build-optimizer --vendor-chunk --extract-css true",
    "build:base-href": "npm run config && ng build --prod --aot --build-optimizer --vendor-chunk --extract-css true --base-href",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "standard-version"
  },
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.1.2",
    "@angular/service-worker": "^6.1.2",
    "class-transformer": "^0.1.9",
    "core-js": "^2.5.4",
    "fs-extra": "^6.0.1",
    "ng2-meta": "^5.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "simple-git": "^1.95.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.1.3",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "dotenv": "^6.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "standard-version": "^4.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  }
}
